{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-nationality-field": "cd nova-components/NationalityField && npm run dev",
        "build-nationality-field-prod": "cd nova-components/NationalityField && npm run prod",
        "build-pending-transactions": "cd nova-components/PendingTransactions && npm run dev",
        "build-pending-transactions-prod": "cd nova-components/PendingTransactions && npm run prod",
        "build-pending-verifications": "cd nova-components/PendingVerifications && npm run dev",
        "build-pending-verifications-prod": "cd nova-components/PendingVerifications && npm run prod",
        "build-pending-verifications-card": "cd nova-components/PendingVerificationsCard && npm run dev",
        "build-pending-verifications-card-prod": "cd nova-components/PendingVerificationsCard && npm run prod",
        "build-pending-transactions-card": "cd nova-components/PendingTransactionsCard && npm run dev",
        "build-pending-transactions-card-prod": "cd nova-components/PendingTransactionsCard && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.19",
        "bootstrap": "^4.1.0",
        "cross-env": "^5.1",
        "jquery": "^3.4.1",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.19",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10",
        "vue2-collapse": "^1.0.15"
    },
    "dependencies": {
        "hamburgers": "^1.1.3",
        "luxon": "^1.28.1",
        "moment": "^2.24.0",
        "pretty-checkbox-vue": "^1.1.9",
        "tailwindcss": "^1.0.5",
        "v-calendar": "^1.0.0-beta.16",
        "vue-datetime": "^1.0.0-beta.10",
        "vue-headful": "^2.0.1",
        "vue-i18n": "^8.14.1",
        "vue-js-modal": "^1.3.31",
        "vue-router": "^3.0.7",
        "vue-toasted": "^1.1.27",
        "vuex": "^3.1.1",
        "weekstart": "^1.0.0"
    }
}
